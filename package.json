{
  "name": "cursornext",
  "version": "0.0.1",
  "main": "index.js",
  "author": "clitetailor",
  "license": "MIT",
  "description": "A minimalist framework for parsing",
  "keywords": [
    "cursor",
    "parser"
  ],
  "scripts": {
    "build": "babel index.ts -o index.js",
    "fmt": "prettier -c .prettierrc.yaml --write index.ts",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/clitetailor/cursornext.git"
  },
  "browserslist": [
    "maintained node versions",
    "not dead"
  ],
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/node": "^12.7.3",
    "ava": "^2.3.0",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2"
  },
  "dependencies": {},
  "bugs": {
    "url": "https://github.com/clitetailor/cursornext/issues"
  },
  "homepage": "https://github.com/clitetailor/cursornext#readme",
  "ava": {
    "files": [
      "test/**/*.ts",
      "!test/helpers/**/*"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "timeout": "2m"
  }
}
